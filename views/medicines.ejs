<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include("./partials/head.ejs") %>
    <link rel="stylesheet" type="text/css" href="/css/medicineCategories.css" />
    <link rel="stylesheet" type="text/css" href="/css/common.style.css" />
    <title>Companies</title>
  </head>
  <body>
    <div class="blurBG"></div>
    <%- include("./partials/navbar.ejs") %>
    <main>
      <div class="blurBG_2"></div>
      <section class="body_containar">
        <h2 class="title">
          <%= meds[0].type %>
          <span style="font-weight: 100; font-size: large"
            >Available brand names</span
          >
        </h2>
        <div class="containar">
          <% meds.map((med)=>{ %>
          <a
            data-tilt
            data-tilt-max="6"
            data-tilt-speed="1000"
            data-tilt-perspective="500"
            href="medicine/<%= med._id %>"
          >
            <div class="smallBlurBG"></div>
            <div class="list">
              <h4><%= med.name %></h4>
              <p><%= med.strength %></p>
              <p><%= med.generic %></p>
              <p><%= med.company %></p>
            </div>
          </a>
          <% }) %>
        </div>
      </section>
    </main>
    <script type="text/javascript" src="/js/vanilla-tilt.js"></script>
  </body>
</html>
